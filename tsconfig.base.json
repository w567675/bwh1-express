{
	"compilerOptions": {
		"moduleResolution": "node",
		"module": "commonjs",
		"target": "es5",
		"strict": true,
		"experimentalDecorators": true,
		"skipLibCheck": true,
		// "noUnusedParameters": true,
		// "noUnusedLocals": true,
		"allowJs": true,
		"outDir": "build",
		"rootDir": "./src"
	},
	"include": [
		"./src/**/*"
	],
	"exclude": [
		"node_modules"
	]
}